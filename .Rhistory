bar_chart <- plot_ly(data = all_years, x = ~Year, y = ~Cases, type = 'bar', name = 'Cases') %>%
add_trace(y = ~Deaths, name = 'Deaths') %>%
layout(title = 'Yearly Sum of Cases and Deaths',
xaxis = list(title = 'Year'),
yaxis = list(title = 'Count'))
